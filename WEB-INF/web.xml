<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <filter>
  	<filter-name>PKIServletFilter</filter-name>
  	<filter-class>is.idega.block.pki.servlet.PKIServletFilter</filter-class>
  </filter>	
  
  <filter-mapping>
  	<filter-name>PKIServletFilter</filter-name>
  	<url-pattern>/pkiauth</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>PKIServletFilter</filter-name>
  	<url-pattern>/pkiauth/</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>PKIServletFilter</filter-name>
  	<url-pattern>/pkiauth*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>PKIServletFilter</filter-name>
  	<url-pattern>/pkiauth/*</url-pattern>
  </filter-mapping>
  
</web-app>